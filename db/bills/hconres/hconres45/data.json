  "introduced_at": "2017-04-04", 
  "number": "45", 
  "official_title": "Expressing the sense of Congress that those who served in the bays, harbors, and territorial seas of the Republic of Vietnam during the period beginning on January 9, 1962, and ending on May 7, 1975, should be presumed to have served in the Republic of Vietnam for all purposes under the Agent Orange Act of 1991.", 
  "popular_title": null, 
  "related_bills": [
    {
      "bill_id": "sconres12-115", 
      "identified_by": "CRS", 
      "reason": "identical", 
      "type": "bill"
    }
  ], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "S001196", 
    "district": "21", 
    "name": "Stefanik, Elise M.", 
    "state": "NY", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2017-04-04", 
  "subjects": [
    "Administrative remedies", 
    "Armed forces and national security", 
    "Asia", 
    "Conflicts and wars", 
    "Department of Veterans Affairs", 
    "Disability and paralysis", 
    "Hazardous wastes and toxic substances", 
