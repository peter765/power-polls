  ], 
  "short_title": "Wounded Officers Recovery Act of 2017", 
  "sponsor": {
    "bioguide_id": "B000213", 
    "district": "6", 
    "name": "Barton, Joe", 
    "state": "TX", 
    "title": "Rep", 
    "type": "person"
