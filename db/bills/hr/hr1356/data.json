  "introduced_at": "2017-03-02", 
  "number": "1356", 
  "official_title": "To amend the Internal Revenue Code of 1986 to permit disclosure of tax return information to the Permanent Select Committee on Intelligence of the House of Representatives and to the Select Committee on Intelligence of the Senate.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
