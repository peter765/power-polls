  "introduced_at": "2017-07-12", 
  "number": "3197", 
  "official_title": "To amend title 38, United States Code, to prohibit the Secretary of Veterans Affairs from conducting medical research causing significant pain or distress to dogs.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Preventing Unkind and Painful Procedures and Experiments on Respected Species Act of 2017", 
  "sponsor": {
    "bioguide_id": "B001290", 
