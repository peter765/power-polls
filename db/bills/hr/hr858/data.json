  "introduced_at": "2017-02-03", 
  "number": "858", 
  "official_title": "To require the Attorney General to make grants to nonprofit organizations to offer legal assistance to certain aliens lawfully admitted for permanent residence, DACA recipients, and refugees, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "DREAMers, Immigrants, and Refugees (DIRe) Legal Aid Act", 
  "sponsor": {
    "bioguide_id": "C001110", 
    "district": "46", 
    "name": "Correa, J. Luis", 
    "state": "CA", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2017-02-03", 
  "subjects": [
    "Detention of persons", 
